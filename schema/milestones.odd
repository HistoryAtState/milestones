            <divGen type="toc"/>

            <div>
                <head>Appendix of TEI Elements, etc.</head>
                <!-- The <schemaSpec> element is in effect the root element for the schema
                specification portion of the ODD. The @ident is required. The @start 
                says what element(s) are allowed as the root element of documents 
                conforming to this schema. There is no official default, but Roma 
                behave as if the default is "teiCorpus TEI".-->
                <schemaSpec ident="milestones" start="TEI" source="tei:2.5.0">

                    <!-- The <moduleRef> elements specify which TEI modules (which
                    correspond directly to chapters in the Guidelines) will be included 
                    in the schema. The value of the @key attribute is the name of the 
                    module. For any element, you can find the name of the module containing 
                    it in the reference documentation for that element. A complete list of 
                    modules is available at
                    http://www.tei-c.org/release/doc/tei-p5-doc/en/html/ST.html#STMA.
                    The first four listed here are required: core, tei, header, 
                    textstructure. -->
                    <moduleRef key="core"
                        include="p hi gloss term name date time ref list item head note 
                        graphic pb lb editor title publisher pubPlace desc"/>
                    <moduleRef key="tei"/>
                    <moduleRef key="header"
                        include="teiHeader fileDesc titleStmt publicationStmt idno sourceDesc 
                        encodingDesc classDecl taxonomy editorialDecl correction revisionDesc listChange change"/>
                    <moduleRef key="textstructure"
                        include="TEI text body div byline dateline closer signed titlePage 
                        docTitle titlePart docImprint docDate front back"/>

                    <moduleRef key="namesdates" include="orgName persName placeName"/>
                    <moduleRef key="figures" include="table row cell figure"/>

                    <!-- including the linking module gives us @corresp -->
                    <moduleRef key="linking" include="anchor"/>
                    <!-- including the transcr module gives us @facs -->
                    <moduleRef key="transcr" include="facsimile space"/>
                    <!-- remove all transcr attributes except for @facs -->
                    <classSpec type="atts" module="linking" ident="att.global.linking" mode="change">
                        <attList>
                            <attDef ident="synch" mode="delete"/>
                            <attDef ident="sameAs" mode="delete"/>
                            <attDef ident="copyOf" mode="delete"/>
                            <attDef ident="next" mode="delete"/>
                            <attDef ident="prev" mode="delete"/>
                            <attDef ident="exclude" mode="delete"/>
                            <attDef ident="select" mode="delete"/>
                        </attList>
                    </classSpec>

                    <!-- kill a few attrs from a class (selective thinning):  -->
                    <classSpec type="atts" ident="att.global" mode="change">
                        <attList>
                            <attDef ident="rendition" mode="delete"/>
                            <attDef ident="xml:space" mode="delete"/>
                            <attDef ident="xml:base" mode="delete"/>
                        </attList>
                    </classSpec>

                    <!-- kill a few more -->
                    <classSpec type="atts" ident="att.breaking" mode="delete"/>
                    <classSpec type="atts" ident="att.declarable" mode="delete"/>
                    <classSpec type="atts" ident="att.declaring" mode="delete"/>
                    <classSpec type="atts" ident="att.divLike" mode="change">
                        <attList>
                            <attDef ident="org" mode="delete"/>
                            <attDef ident="sample" mode="delete"/>
                            <attDef ident="part" mode="delete"/>
                        </attList>
                    </classSpec>
                    <classSpec type="atts" ident="att.docStatus" mode="delete"/>
                    <classSpec type="atts" ident="att.editLike" mode="delete"/>
                    <classSpec type="atts" ident="att.fragmentable" mode="delete"/>
                    <classSpec type="atts" ident="att.internetMedia" mode="delete"/>
                    <classSpec type="atts" ident="att.naming" mode="change">
                        <attList>
                            <attDef ident="nymRef" mode="delete"/>
                        </attList>
                    </classSpec>
                    <classSpec type="atts" ident="att.personal" mode="delete"/>

                    <!-- 
                    constrain tei:list/@type
                -->
                    <elementSpec ident="list" module="core" mode="change">
                        <desc>Restrict @type values for tei:list elements</desc>
                        <attList>
                            <attDef ident="type" usage="req" mode="replace">
                                <valList type="closed">
                                    <valItem ident="participants">
                                        <gloss>Document participants List</gloss>
                                    </valItem>
                                    <valItem ident="subject">
                                        <gloss>Document subject heading</gloss>
                                    </valItem>
                                    <valItem ident="index">
                                        <gloss>Back of book Index</gloss>
                                    </valItem>
                                    <valItem ident="terms">
                                        <gloss>Front matter List of Terms &amp;
                                            Abbreviations</gloss>
                                    </valItem>
                                    <valItem ident="names">
                                        <gloss>Front matter List of Persons</gloss>
                                    </valItem>
                                    <valItem ident="toc">
                                        <gloss>Front matter Table of Contents</gloss>
                                    </valItem>
                                    <valItem ident="sources">
                                        <gloss>Front matter List of Sources</gloss>
                                    </valItem>
                                    <valItem ident="to">
                                        <gloss>Document Recipients List</gloss>
                                    </valItem>
                                    <valItem ident="simple">
                                        <gloss>Tightly-spaced text</gloss>
                                        <!-- TEI supplies this definition for "simple": -->
                                    </valItem>
                                    <valItem ident="references">
                                        <gloss>Document References heading</gloss>
                                    </valItem>
                                </valList>
                            </attDef>
                        </attList>
                    </elementSpec>

                    <!-- 
                    constrain tei:div/@type
                -->
